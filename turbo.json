{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "inputs": ["src/**", "tsup.config.ts"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": ["coverage"]
    }
  },
  "globalDependencies": ["tsconfig.base.json"]
}
