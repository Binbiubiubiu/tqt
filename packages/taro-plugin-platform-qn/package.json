{
  "name": "@tqtjs/taro-plugin-platform-qn",
  "version": "0.0.1",
  "description": "淘宝PC小程序平台插件",
  "author": "Binbiubiubiu <2632003786@qq.com>",
  "license": "MIT",
  "keywords": [
    "tqtjs",
    "taro",
    "taobao",
    "platform-plugin",
    "mini-program",
    "pc"
  ],
  "main": "index.js",
  "module": "index.js",
  "types": "types/index.d.ts",
  "typesVersions": {
    "*": {
      "shims-qn": [
        "./types/shims-qn.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./index.js"
    },
    "./shims-qn": "./types/shims-qn.d.ts",
    "./*": "./*"
  },
  "files": [
    "index.js",
    "dist",
    "types"
  ],
  "scripts": {
    "build": "pnpm build:lib",
    "build:lib": "tsup",
    "test": "tsd",
    "dev": "pnpm build:lib --watch src"
  },
  "dependencies": {
    "@tqtjs/shared": "workspace:*"
  },
  "peerDependencies": {
    "@tarojs/components": "^3.3.8",
    "@tarojs/taro": "^3.3.8",
    "@tarojs/service": "^3.3.8",
    "@tarojs/shared": "^3.3.8"
  },
  "repository": "git+https://github.com/Binbiubiubiu/tqtjs.git",
  "bugs": {
    "url": "https://github.com/Binbiubiubiu/tqtjs/issues"
  },
  "homepage": "https://github.com/Binbiubiubiu/tqtjs#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}