{
  "name": "@tqtjs/shims",
  "version": "0.0.1",
  "description": "Some types of shims",
  "author": "Binbiubiubiu <2632003786@qq.com>",
  "license": "MIT",
  "keywords": [
    "tqtjs",
    "taro",
    "taobao",
    "platform-plugin",
    "mini-program"
  ],
  "type": "module",
  "files": [
    "index.d.ts",
    "taro.qn.d.ts",
    "taro.tb.d.ts",
    "qn",
    "tb",
    "global.d.ts"
  ],
  "main": "",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "qn": [
        "./taro.qn.d.ts"
      ],
      "qn/*": [
        "./qn/*"
      ],
      "tb": [
        "./taro.tb.d.ts"
      ],
      "tb/*": [
        "./tb/*"
      ]
    }
  },
  "exports": {
    "./qn": "./taro.qn.d.ts",
    "./qn/*": "./qn/*",
    "./tb": "./taro.tb.d.ts",
    "./tb/*": "./tb/*"
  },
  "scripts": {
    "test": "tsd"
  },
  "dependencies": {
    "@types/react": "*"
  },
  "devDependencies": {
    "tsd": "^0.26.1"
  },
  "peerDependencies": {
    "@tarojs/components": "^3.3.8",
    "@tarojs/taro": "^3.3.8"
  },
  "peerDependenciesMeta": {
    "@tarojs/taro": {
      "optional": true
    },
    "@tarojs/components": {
      "optional": true
    }
  },
  "tsd": {
    "directory": "test-d",
    "compilerOptions": {
      "strict": true,
      "jsx": "react",
      "target": "esnext",
      "lib": [
        "esnext",
        "dom",
        "dom.iterable"
      ],
      "module": "esnext",
      "moduleResolution": "node"
    }
  },
  "repository": "git+https://github.com/Binbiubiubiu/tqtjs.git",
  "bugs": {
    "url": "https://github.com/Binbiubiubiu/tqtjs/issues"
  },
  "homepage": "https://github.com/Binbiubiubiu/tqtjs#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}